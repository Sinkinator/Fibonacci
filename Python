def print_hi(name):
    # Use a breakpoint in the code line below to debug your script.
    print(f'Hi, {name}')  # Press Ctrl+F8 to toggle the breakpoint.


# Press the green button in the gutter to run the script.
if __name__ == '__main__':
    print_hi('PyCharm')
    fib_lista = [1, 1]
    print("Adjuk meg hányadik elemig kérjük a Fibonácsí sorozatot!")
    elemszam = int(input())

    uj_elem = ''
    i = 2
    while i < elemszam:
        uj_elem = fib_lista[i-1] + fib_lista[i-2]

        fib_lista.append(uj_elem)
        i = i + 1
    print(fib_lista)

